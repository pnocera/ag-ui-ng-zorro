            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ag-ui/core/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "@ag-ui/theming": {
      "projectType": "library",
      "root": "projects/ag-ui/theming",
      "sourceRoot": "projects/ag-ui/theming/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ag-ui/theming/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ag-ui/theming/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
