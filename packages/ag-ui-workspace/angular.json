{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@ag-ui/ng-zorro": {
      "projectType": "library",
      "root": "projects/ag-ui/ng-zorro",
      "sourceRoot": "projects/ag-ui/ng-zorro/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ag-ui/ng-zorro/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ag-ui/ng-zorro/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ag-ui/ng-zorro/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "@ag-ui/core": {
      "projectType": "library",
      "root": "projects/ag-ui/core",
      "sourceRoot": "projects/ag-ui/core/src",
      "prefix": "lib",
